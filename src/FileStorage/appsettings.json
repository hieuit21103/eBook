{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "PgHost": "localhost",
    "PgPort": "5432",
    "PgUsername": "postgres",
    "PgPassword": "postgres"
  },
  "S3": {
    "BucketName": "document",
    "PresignExpiration": "15",
    "AccessKey": "your-access-key",
    "SecretKey": "your-secret-key",
    "EndPoint": "https://s3.amazonaws.com"
  }
}
